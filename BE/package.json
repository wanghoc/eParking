{
  "name": "eparking-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prisma": "node server-prisma.js",
    "dev": "nodemon server.js",
    "dev:prisma": "nodemon server-prisma.js",
    "db:init": "mysql -u root -p < schema.sql",
    "db:init:docker": "mysql -h localhost -P 3306 -u eparking_user -p < schema-mysql.sql",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "prisma db push",
    "prisma:db:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "build-src": "babel src -d build --copy-files",
    "docker:build": "docker build -t eparking-backend .",
    "docker:run": "docker run -p 5000:5000 --env-file ../.env eparking-backend",
    "docker:dev": "docker-compose -f ../docker-compose.yml up --build backend database"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "eParking backend server",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "mysql2": "^3.14.3",
    "pg": "^8.12.0",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
